{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/js/**",
        "destination": "/js/"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
